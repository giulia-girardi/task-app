<div class="app">
    <%- include('./partials/navbar.ejs') %>
    <div class="pagecontent">
        <div class="tabtitle">
            <h2>Completed Tasks</h2>
        </div>
        <div class="tabpage">
        <h3>Your tasks</h3>
        <% pastTasks.forEach(task => { %>
            <p>Task Name: <%= task.taskName  %> </p>    
            <p>Due date: <%= task.dueDate.toDateString()  %> </p>
        <% }) %>


        <h3>Tasks shared with you</h3>
        <% sharedTasksDone.forEach(task => { %>
            <p>Task Name: <%= task.taskName  %> </p>    
            <p>Due date: <%= task.dueDate.toDateString()  %> </p>
            <p>Collaborators: <%= task.collaborators  %> </p>
        <% }) %>
        </div>
    </div>
</div>
